        integrity="sha512-gZwIG9x3wUXg2hdXF6+rVkLF/0Vi9U8D2Ntg4Ga5I5BZpVkVxlJWbSQtXPSiUTtC0TjtGOmxa1AJPuV0CPthew=="
        crossorigin=""></script>
        {% endblock %}
    </head>

    {% block header %}

             <!--LOGO-->
            <img src="{{ asset('images/logo.png')}}" alt="Logo" title="Logo du site" id="logo">
            <!-- TITRE -->
            <h1>HELP'ELLE<br>l'annuaire des associations<br> d'aide aux femmes</h1>

            <!--VISAGE FEMME-->
            <img src="{{ asset('images/visagefemme.png')}}" alt="Photo Femme" title="Photo visage femme" id="visage">

            <nav>
               <ul>
                     <li><a href="{{ path('accueil')}}">Accueil</a></li>
                     <li><a href="{{ path('formulaire')}}">Formulaire</a></li>
                     <li><a href="{{ path('annuaire')}}">Annuaire</a></li>
                     <li><a href="{{ path('conseils')}}">Conseils</a></li>
                     <li><a href="{{ path('prevention')}}">Prévention</a></li>
                     <li><a href="{{ path('inscrire_mon_association')}}">Inscrire mon association</a></li>
                     <li><a href="{{ path('mon_compte')}}">Mon compte</a></li>
                </ul>

            </nav>
                  <li class="nav-item active">
                        {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                        <em> Bonjour {{ app.user.Nom }}</em>  <a href="{{ path('app_login')}}" class="btn">Se déconnecter</a>
                        {% else %}
                        <a href="{{ path('app_login')}}" class="btn">Se connecter</a>   
                        <a href="{{ path('app_register')}}" class="btn">S'inscrire</a>
                        {% endif %}
                    </li>


        {% endblock %}
   
           {% block body %}
           
           
           {% endblock %}
        {% block footer %}
         <footer>
            <p>&copy;2020 Equipe Help'Elle - Tous droits réservés</p>
        </footer>
        {% endblock %}
        {% block javascripts %}
        
        {% endblock %}

    </body>
</html>
